
<div id="card-container" class="mt-4 mb-4 z-0">
  <div class="text-center py-4 font-bold font-serif text-4xl"><%= title %></div>
  <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-4 mb-4">
    <% articles.forEach((article) => { %>
      <div class="p-4 bg-white shadow-xl rounded-lg" style="height: 75vh">
        <%- include('NewspaperCard', { article: article }) %>
      </div>
    <% }) %>
  </div>
</div>
