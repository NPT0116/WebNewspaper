{
  "name": "webnewspaper",
  "version": "1.0.0",
  "description": "\"# WebNewspaper\"",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run tailwind:css\" \"npx nodemon\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "rimraf ./dist && tsc && tsc-alias",
    "seed:build": "npm run build && node dist/seeds/indexSeed.js",
    "start": "node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint-staged": "lint-staged",
    "tailwind:css": "postcss public/css/tailwind.css -o public/css/styles.css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.693.0",
    "@aws-sdk/client-s3": "^3.693.0",
    "@aws-sdk/client-sns": "^3.693.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "ckeditor5": "^43.3.1",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.0",
    "mongoose": "^8.8.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-local": "^1.0.0",
    "postcss-cli": "^11.0.0",
    "rimraf": "^6.0.1",
    "socket-client": "^1.0.0",
    "socket.io": "^4.8.1",
    "tsc-alias": "^1.8.10"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/connect-flash": "^0.0.40",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.0",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^22.9.0",
    "@types/passport": "^1.0.17",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-local": "^1.0.38",
    "@types/socket.io": "^3.0.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "eslint": "^9.14.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "prettier-plugin-ejs": "^1.0.3",
    "tailwindcss": "^3.4.15",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.14.0",
    "vite": "^5.4.11"
  },
  "type": "module",
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format",
      "git add ."
    ]
  }
}
